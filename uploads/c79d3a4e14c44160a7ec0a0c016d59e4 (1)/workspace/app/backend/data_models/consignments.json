{
    "title": "consignments",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "description": "Primary key, auto-increment"
        },
        "user_id": {
            "type": "string",
            "description": "Affiliate user ID"
        },
        "affiliate_id": {
            "type": "string",
            "description": "Affiliate user ID (for RLS)"
        },
        "inventory_id": {
            "type": "integer",
            "description": "Reference to inventory"
        },
        "quantity": {
            "type": "integer",
            "description": "Quantity consigned"
        },
        "consigned_date": {
            "type": "string",
            "format": "date-time",
            "description": "Date consigned"
        },
        "return_date": {
            "type": "string",
            "format": "date-time",
            "description": "Date returned"
        },
        "status": {
            "type": "string",
            "enum": [
                "PENDING",
                "CONFIRMED",
                "PARTIAL",
                "RETURNED"
            ],
            "description": "Consignment status"
        },
        "notes": {
            "type": "string",
            "description": "Additional notes"
        },
        "created_at": {
            "type": "string",
            "format": "date-time",
            "description": "Creation timestamp"
        },
        "updated_at": {
            "type": "string",
            "format": "date-time",
            "description": "Last update timestamp"
        },
        "deleted_at": {
            "type": "string",
            "format": "date-time",
            "description": "Soft delete timestamp"
        }
    },
    "required": [
        "id",
        "user_id",
        "affiliate_id",
        "inventory_id",
        "quantity",
        "status"
    ]
}